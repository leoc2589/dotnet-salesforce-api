{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Salesforce": {
    "Account": {
      "BaseUri": "https://{domainName}.my.salesforce.com",
      "ApiVersion": "vXX.X",
      "ClientId": "{clientId}",
      "ClientSecret": "{clientSecret}"
    },
    "Credentials": {
      "Username": "{username}",
      "Password": "{password}"
    }
  }
}